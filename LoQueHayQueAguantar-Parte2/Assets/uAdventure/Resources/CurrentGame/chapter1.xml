  <graph-conversation id="ConverPortero">
    <dialogue-node nodeindex="0" editor-x="51" editor-y="14">
      <speak-char idTarget="Recepcionista">
        <resources name="No name" />
        <text>Buenos días, ¿necesita ayuda?</text>
      </speak-char>
      <condition />
      <speak-player>
        <resources name="No name" />
        <text>&lt;&lt; ¿No se acuerda de mí? Casi que mejor, la verdad. &gt;&gt;</text>
      </speak-player>
      <condition />
      <speak-player>
        <resources name="No name" />
        <text>Hola. Sí, me han llamado esta mañana para que me incorporase al nuevo puesto de programador junior.</text>
      </speak-player>
      <condition />
      <speak-char idTarget="Recepcionista">
        <resources name="No name" />
        <text>Perfecto. Déjeme que llame al señor Manjón, él se encargará de enseñarle todo.</text>
      </speak-char>
      <condition />
      <speak-char idTarget="Recepcionista">
        <resources name="No name" />
        <text>¿Cómo se llama usted?</text>
      </speak-char>
      <condition />
      <speak-player>
        <resources name="No name" />
        <text>Alexis Fernández</text>
      </speak-player>
      <condition />
      <speak-char idTarget="Recepcionista">
        <resources name="No name" />
        <text>Ok, espere un momento...</text>
      </speak-char>
      <condition />
      <child nodeindex="1" />
      <effect>
        <show-text x="300" y="300" frontColor="#FFFFFFFF" borderColor="#000000FF" uri="">&lt;&lt;Se escucha el sonido del teléfono&gt;&gt;</show-text>
        <condition />
      </effect>
    </dialogue-node>
    <dialogue-node nodeindex="1" editor-x="92" editor-y="340">
      <speak-char idTarget="Recepcionista">
        <resources name="No name" />
        <text>Señor Manjón, ha llegado Alexis Fernández. </text>
      </speak-char>
      <condition />
      <speak-char idTarget="Recepcionista">
        <resources name="No name" />
        <text>...</text>
      </speak-char>
      <condition />
      <speak-char idTarget="Recepcionista">
        <resources name="No name" />
        <text>Vale, perfecto</text>
      </speak-char>
      <condition />
      <child nodeindex="2" />
      <effect>
        <show-text x="300" y="300" frontColor="#FFFFFFFF" borderColor="#000000FF" uri="">&lt;&lt;El portero cuelga el teléfono&gt;&gt;</show-text>
        <condition />
      </effect>
    </dialogue-node>
    <dialogue-node nodeindex="2" editor-x="157" editor-y="587">
      <speak-char idTarget="Recepcionista">
        <resources name="No name" />
        <text>Muy bien, estará aquí en un momento. Sea paciente.</text>
      </speak-char>
      <condition />
      <speak-player>
        <resources name="No name" />
        <text>Perfecto, muchas gracias.</text>
      </speak-player>
      <condition />
      <child nodeindex="3" />
      <effect>
        <activate flag="PorteroHablado" />
        <condition />
      </effect>
    </dialogue-node>
    <dialogue-node nodeindex="3" editor-x="86" editor-y="851">
      <speak-char idTarget="Balta">
        <resources name="No name" />
        <text>¡Señorita Alexis!</text>
      </speak-char>
      <condition />
      <speak-char idTarget="Balta">
        <resources name="No name" />
        <text>Buenos días, ha sido usted puntual, venga por aquí, subiremos en ascensor.</text>
      </speak-char>
      <condition />
      <child nodeindex="4" />
    </dialogue-node>
    <option-node nodeindex="4" keepShowing="yes" preListening="yes" editor-x="193" editor-y="1149" x="193" y="1149">
      <speak-player>
        <text>¡Voy!</text>
        <resources name="No name" />
      </speak-player>
      <condition />
      <child nodeindex="5" />
    </option-node>
    <dialogue-node nodeindex="5" editor-x="552" editor-y="1158">
      <end-conversation>
